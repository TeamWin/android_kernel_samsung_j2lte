	fimc_is_module_4h5yc: fimc-is_sensor_4h5yc@0 {
		compatible = "samsung,sensor-module-4h5yc";

		pinctrl-names = "pin0", "pin1";
		pinctrl-0 = <&fimc_is_mclk0_fn>; /* rear on */
		pinctrl-1 = <&fimc_is_mclk0_in>; /* rear off */

		position = <0>; /* Rear:0. Front:1 */
		id = <0>; /* bns_id */
		mclk_ch = <0>;
		sensor_i2c_ch = <0>; /* I2C 0 */
		sensor_i2c_addr = <0x37>; /* need the i2c_addr modify */

		gpio_reset = <&gpd0 2 0x1>; /* sensor reset */
		status = "okay";

		af {
			product_name = <10>; /* 10: ACTUATOR_NAME_ZC533 */
			i2c_addr = <0x1C>;
			i2c_ch = <0>; /* I2C 0 */
		};

		flash {
			product_name = <8>; /* FLADRV_NAME_RT5033 */
			flash_first_gpio = <2>;
			flash_second_gpio = <1>;
		};
	};

	fimc_is_module_5e3: fimc-is_sensor_5e3@0 {
